PROC TVCOLORS
I=GRCHAN; DOCOLOR=1; PLCOLOR=0
IF (GRCHAN > 0) THEN;GREAD;PLCOLOR=RGBCOL,RGBCOL,RGBCOL,RGBCOL
ELSE;FOR GRCHAN=4:1 BY -1;GREAD;PLCOLOR(1,GRCHAN)~RGBCOL; END
END; PLCOLOR(1,9)~RGBCOL; GRCHAN=I; RETURN
FINISH
